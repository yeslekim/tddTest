	private MembershipController target;
	
	private MockMvc mockMvc;
	private Gson gson;
	
	@Test
	public void mockMvc가Null이아님() throws Exception {
		assertThat(target).isNotNull();
		assertThat(mockMvc).isNotNull();
	}
}
